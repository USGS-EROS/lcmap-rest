#!/bin/bash

LCMAP_ACCEPT_HDR="Accept: application/vnd.usgs.lcmap.v0.5+json, application/problem+json"
LCMAP_HOST="http://localhost:1077"
DELAY="1"
YEAR="2016"

JOB=$(curl -s -v \
  -H "$LCMAP_ACCEPT_HDR" \
  -H "$LCMAP_TOKEN_HDR" \
  -X POST "$LCMAP_HOST/api/models/sample/os-process?delay=$DELAY&year=$YEAR")

echo $JOB
